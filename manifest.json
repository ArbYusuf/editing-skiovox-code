{
  "manifest_version": 2,
  "name": "Bookmark Shortcut",
  "version": "1.0",
  "description": "A simple extension to bookmark active tab with shortcut Control + D",
  "permissions": ["tabs", "bookmarks"],
  "background": {
    "scripts": ["script.js"],
    "persistent": false
  },
  "commands": {
    "bookmark_shortcut": {
      "suggested_key": {
        "default": "Ctrl+D"
      },
      "description": "Bookmark the active tab"
    }
  },
  "manifest_key": "value"
}
